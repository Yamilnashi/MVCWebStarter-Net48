    @*<link rel="stylesheet" href="https://california.azureedge.net/cdt/statetemplate/5.5.24/css/colorscheme-oceanside.min.css" />*@
    @*<link rel="stylesheet" href="https://california.azureedge.net/cdt/statetemplate/5.5.24/css/colorscheme-orangecounty.min.css" />*@
    @*<link rel="stylesheet" href="https://california.azureedge.net/cdt/statetemplate/5.5.24/css/colorscheme-pasorobles.min.css" />*@
    @*<link rel="stylesheet" href="https://california.azureedge.net/cdt/statetemplate/5.5.24/css/colorscheme-santabarbara.min.css" />*@
    @*<link rel="stylesheet" href="https://california.azureedge.net/cdt/statetemplate/5.5.24/css/colorscheme-santacruz.min.css" />*@
    @*<link rel="stylesheet" href="https://california.azureedge.net/cdt/statetemplate/5.5.24/css/colorscheme-shasta.min.css" />*@
