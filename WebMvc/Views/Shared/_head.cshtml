@{ 
    string departmentName = WebMvc.Data.Utils.GetAppSetting(WebMvc.Data.EAppSettings.DEPARTMENT_NAME);
}
<head>
    <meta charset="utf-8">
    <title>@ViewData["Title"] - @departmentName</title>
    <link rel="icon" type="image/x-icon" href="~/Content/icons/favicon.ico">
    <meta name="Author" content="State of California" />
    <meta name="Description" content="State of California" />
    <meta name="Keywords" content="California, government" />
    <!-- Use highest compatibility mode -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- http://t.co/dKP3o1e -->
    <meta name="HandheldFriendly" content="True">
    <!-- for Blackberry, AvantGo -->
    <meta name="MobileOptimized" content="320">
    <!-- for Windows mobile -->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0">

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <meta property="og:image" content="http://https://template.webstandards.ca.gov/template.jpg">
    <meta property="og:image:type" content="image/png">
    <meta property="og:image:width" content="1024">
    <meta property="og:image:height" content="1024">

    @Styles.Render("~/bundles/googlefont")
    @Styles.Render("~/bundles/bootstrapicons")

    @*State Template core styles*@
    @Styles.Render("~/bundles/statecore")
    @*State Template color scheme*@
    <link rel="stylesheet" href="https://california.azureedge.net/cdt/statetemplate/5.5.24/css/colorscheme-sacramento.min.css" />
    @*<link rel="stylesheet" href="https://california.azureedge.net/cdt/statetemplate/5.5.24/css/colorscheme-delta.min.css" />*@
